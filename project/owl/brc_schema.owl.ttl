brc:DatasetCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DatasetCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:datasets ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:Dataset ;
            owl:onProperty brc:datasets ] ;
    skos:definition "Container class for defining a collection of datasets." ;
    skos:inScheme brc:brc_schema .

brc:Dataset a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Dataset" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom brc:Individual ;
            owl:onProperty brc:creator ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:identifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:RepositoryEnum ;
            owl:onProperty brc:repository ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:datasetName ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:brc ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty brc:identifier ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:keywords ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:repository ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:Organism ;
            owl:onProperty brc:species ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:species ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty brc:brc ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:datasetName ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:identifier ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:datasetName ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:keywords ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:bibliographicCitation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uri ;
            owl:onProperty brc:bibliographicCitation ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:BRCEnum ;
            owl:onProperty brc:brc ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:has_related_ids ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:date ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty brc:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:unionOf ( brc:AnalysisType linkml:String ) ] ;
            owl:onProperty brc:analysisType ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:analysisType ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:RelatedItem ;
            owl:onProperty brc:relatedItem ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:repository ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:date ;
            owl:onProperty brc:date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:relatedItem ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:bibliographicCitation ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty brc:has_related_ids ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty brc:date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:analysisType ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty brc:creator ] ;
    skos:closeMatch dcat:Catalog ;
    skos:definition "A dataset containing metabolomics and proteomics data." ;
    skos:exactMatch schema1:Dataset,
        dcat:Dataset,
        bald:Container ;
    skos:inScheme brc:brc_schema .

brc:Individual a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Individual" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:creatorName ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:affiliation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:primaryContact ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Boolean ;
            owl:onProperty brc:primaryContact ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:primaryContact ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:creatorName ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:unionOf ( brc:Organization linkml:String ) ] ;
            owl:onProperty brc:affiliation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:affiliation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:creatorName ] ;
    skos:definition "An individual involved in the dataset." ;
    skos:exactMatch schema1:Person ;
    skos:inScheme brc:brc_schema .

brc:Organism a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Organism" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:NCBITaxID ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:NCBITaxID ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:scientificName ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:scientificName ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty brc:NCBITaxID ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:scientificName ] ;
    skos:definition "An organism studied in the dataset." ;
    skos:inScheme brc:brc_schema .

brc:RelatedItem a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "RelatedItem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:relatedItemIdentifier ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:relatedItemIdentifier ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty brc:relatedItemType ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty brc:relatedItemIdentifier ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom brc:CitedItemType ;
            owl:onProperty brc:relatedItemType ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty brc:title ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty brc:relatedItemType ] ;
    skos:definition "A related publication or item, including cited publications." ;
    skos:inScheme brc:brc_schema .

brc:ror_identifier a owl:Class,
