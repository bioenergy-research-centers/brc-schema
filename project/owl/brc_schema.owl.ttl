brc:creator a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "creator" ;
    skos:definition "List of creators involved in the dataset, where one must be the primary contact." ;
    skos:inScheme brc:brc_schema .

brc:datasets a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "datasets" ;
    skos:definition "List of datasets in the collection." ;
    skos:inScheme brc:brc_schema .

brc:has_related_ids a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has_related_ids" ;
    skos:definition "\"Related identifiers for the dataset. These should be identifiers to records in other repositories, and these records may be the same data or components of the dataset.\"" ;
    skos:inScheme brc:brc_schema .

brc:keywords a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "keywords" ;
    skos:definition "Keywords associated with the dataset." ;
    skos:exactMatch schema1:keywords ;
    skos:inScheme brc:brc_schema .

brc:relatedItem a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "relatedItem" ;
    skos:definition "Related publications or items." ;
