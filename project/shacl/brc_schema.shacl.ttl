            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 77 ;
            sh:path osti:report_type_other ;
            sh:pattern "^.{0,80}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Information regarding physical size of media or report, if applicable." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 62 ;
            sh:path osti:product_size ;
            sh:pattern "^.{0,50}$" ],
        [ sh:class osti:Organization ;
            sh:description "List of organizations related to this record. For submissions, at least SPONSOR and RESEARCHING organization is required." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 53 ;
            sh:path osti:organizations ],
        [ sh:datatype xsd:string ;
            sh:description "Date of OUO access limitation expiration if applicable." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 55 ;
            sh:path osti:ouo_release_date ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 74 ;
            sh:path osti:report_period_end_date ],
        [ sh:datatype xsd:string ;
            sh:description "Any comments made by the releasing official on the record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 73 ;
            sh:path osti:releasing_official_comments ],
        [ sh:datatype xsd:string ;
            sh:description "A three-character field constrained to 'I', 'II', 'IIA', 'IIB', or 'III' indicating the phase of this SBIR/STTR report." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
