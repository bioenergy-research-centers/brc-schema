osti:RelatedIdentifier a sh:NodeShape ;
    sh:closed true ;
    sh:description "Identifies other resources that are related in some manner to this record" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Identify the type of this related identifier" ;
            sh:in ( "ARK" "arXiv" "bibcode" "DOI" "EAN13" "EISSN" "IGSN" "ISBN" "ISSN" "ISTC" "Handle" "LISSN" "LSID" "OTHER" "PMCID" "PMID" "PURL" "UPC" "URI" "URL" "URN" "UUID" "w3id" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path osti:type ],
        [ sh:in ( "BasedOnData" "Cites" "Compiles" "Continues" "Describes" "Documents" "Finances" "HasComment" "HasDerivation" "HasMetadata" "HasPart" "HasRelatedMaterial" "HasReply" "HasReview" "HasVersion" "IsBasedOn" "IsBasisFor" "IsCitedBy" "IsCommentOn" "IsCompiledBy" "IsContinuedBy" "IsDataBasisFor" "IsDerivedFrom" "IsDescribedBy" "IsDocumentedBy" "IsFinancedBy" "IsIdenticalTo" "IsMetadataFor" "IsNewVersionOf" "IsObsoletedBy" "IsOriginalFormOf" "IsPartOf" "IsPreviousVersionOf" "IsReferencedBy" "IsRelatedMaterial" "IsReplyTo" "IsRequiredBy" "IsReviewedBy" "IsReviewOf" "IsSourceOf" "IsSupplementedBy" "IsSupplementTo" "IsVariantFormOf" "IsVersionOf" "Obsoletes" "References" "Requires" "Reviews" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path osti:relation ;
            sh:pattern "^.{0,20}$" ],
        [ sh:datatype xsd:string ;
            sh:description "The value of the identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path osti:value ;
            sh:pattern "^.{0,2000}$" ] ;
    sh:targetClass osti:RelatedIdentifier .

brc:Organism a sh:NodeShape ;
    sh:closed true ;
    sh:description "An organism studied in the dataset." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Scientific name of the organism." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path brc:scientificName ],
        [ sh:datatype xsd:integer ;
            sh:description "NCBI taxonomy ID for the organism." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path brc:NCBITaxID ] ;
    sh:targetClass brc:Organism .

brc:BRCOrganization a sh:NodeShape ;
    sh:closed true ;
    sh:description "An organization involved in the dataset. The name denotes this is the BRC-specific model of an organization, rather than that defined by OSTI, though the classes are similar." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class brc:BRCOrganization ;
            sh:description "Higher-level parent of this organization." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path brc:parentOrganization ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the organization." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:name ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Wikidata identifier for the organization." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:wikidata_id ],
        [ sh:datatype xsd:anyURI ;
            sh:description "ROR identifier for the organization." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path brc:ror_id ] ;
    sh:targetClass brc:BRCOrganization .

