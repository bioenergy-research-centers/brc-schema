            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 45 ;
            sh:path osti:audit_logs ],
        [ sh:datatype xsd:string ;
            sh:description "Additional information for 'OTHER' product types.  Required if 'OT' product type is specified for this record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 64 ;
            sh:path osti:product_type_other ;
            sh:pattern "^.{0,200}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Code representing the type of conference-related work of this record. Generally, only applicable to CO type submissions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path osti:conference_type ;
            sh:pattern "^.{0,1}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates date on which the document embargo ends, if applicable." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 38 ;
            sh:path osti:media_embargo_sunset_date ],
        [ sh:datatype xsd:string ;
            sh:description "Additional information about access/distribution limitation for this record, if needed. Required for CUI or PDOUO designations in access limitation. May contain information about the following: Special handling instructions, Copyright restrictions, Other criteria pertinent to the review, access limitation, announcement, and/or restriction of this STI product." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path osti:access_limitation_other ],
        [ sh:datatype xsd:string ;
