@prefix ERO: <http://purl.obolibrary.org/obo/ERO_> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix MI: <http://purl.obolibrary.org/obo/MI_> .
@prefix NCIT: <http://purl.obolibrary.org/obo/NCIT_> .
@prefix OBI: <http://purl.obolibrary.org/obo/OBI_> .
@prefix brc: <https://w3id.org/brc/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix osti: <https://www.osti.gov/biblio/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ror: <https://ror.org/> .
@prefix schema1: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

brc:DatasetCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "Container class for defining a collection of datasets." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class schema1:Dataset ;
            sh:description "List of datasets in the collection." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path brc:datasets ],
        [ sh:datatype xsd:string ;
            sh:description "Version of the schema used for the collection." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path brc:schema_version ] ;
    sh:targetClass brc:DatasetCollection .

osti:Records a sh:NodeShape ;
    sh:closed true ;
    sh:description "A list of Record metadata." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class osti:Record ;
            sh:description "List of records in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path osti:records ] ;
    sh:targetClass osti:Records .

schema1:Dataset a sh:NodeShape ;
    sh:closed true ;
    sh:description "A body of structured information describing some topic or topics of interest. This includes metadata about the dataset." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The primary Bioenergy Research Center affiliation. This is a single BRC name." ;
            sh:in ( "CABBI" "CBI" "GLBRC" "JBEI" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 7 ;
            sh:path prov:wasAttributedTo ],
        [ sh:datatype xsd:string ;
            sh:description "A detailed description of the dataset." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path dcterms:description ],
        [ sh:description "The repository where the dataset is stored." ;
            sh:in ( "AmeriFlux" ror:04pvg6f08 ror:00x6h5n95 ror:041mxqs23 "GenBank" "GEO" "GitHub" "GLBRC Sustainability" "Iowa State University FigShare" "ICE" "Illinois Data Bank" "iProX" "JGI Gold" "JGI Genome Portal" "jPOST" "MassIVE" ror:01t2a8a42 ror:05cwx3318 "NCBI BioProject" "NCBI SRA" "ORNL DAAC" ror:031478740 "PanoramaPublic" "PedtideAtlas" "PRIDE" "Protein Data Bank" ror:00zbfm828 "Zenodo" ) ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path brc:repository ],
        [ sh:datatype xsd:date ;
            sh:description "The date the dataset was created or published." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dcterms:date ],
        [ sh:class brc:Organism ;
            sh:description "Species information for the organism(s) studied." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 13 ;
            sh:path brc:species ],
        [ sh:datatype xsd:boolean ;
            sh:defaultValue true ;
            sh:description "Indicates whether the dataset is active or inactive. This is a boolean field - true indicates active, false indicates inactive." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path brc:active ],
        [ sh:description "Additional Bioenergy Research Center affiliations. This is a list of one or more additional BRC names, for instances in which the dataset is associated with multiple centers." ;
            sh:in ( "CABBI" "CBI" "GLBRC" "JBEI" ) ;
            sh:order 8 ;
            sh:path prov:wasAttributedTo ],
        [ sh:datatype xsd:boolean ;
            sh:description "Indicates whether availability of the dataset has encountered some inconsistency. This is a boolean field - true indicates alert, false indicates no alert. For example, if we have a Dataset object but the Dataset is missing from its source feed, this should be set to true." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:alert ],
        [ sh:class brc:RelatedItem ;
            sh:description "Related publications or items." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 18 ;
            sh:path brc:relatedItem ],
        [ sh:class brc:Contributor ;
            sh:description "Contributors to the dataset who are not necessarily authors." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path brc:contributors ],
        [ sh:datatype xsd:anyURI ;
            sh:description "\"Related identifiers for the dataset. These should be identifiers to records in other repositories, and these records may be the same data or components of the dataset.\"" ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path brc:has_related_ids ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for the dataset." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path schema1:identifier ],
        [ sh:datatype xsd:string ;
            sh:description "\"Name of a overall dataset to which this data entry belongs.\"" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path brc:datasetName ],
        [ sh:class schema1:Person ;
            sh:description "List of creators involved in the dataset, where one must be the primary contact." ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path dcterms:creator ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Citation for the dataset." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path dcterms:bibliographicCitation ],
        [ sh:defaultValue "not specified"^^xsd:string ;
            sh:description "The type of analysis performed on the dataset." ;
            sh:maxCount 1 ;
            sh:or ( [ sh:in ( MI:0004 OBI:0000800 NCIT:C19771 NCIT:C188690 NCIT:C17606 "Ms_imaging" ERO:0001660 "srm_mrm" OBI:0002958 OBI:0001899 ) ] [ sh:datatype xsd:string ;
                        sh:nodeKind sh:Literal ] ) ;
            sh:order 15 ;
            sh:path brc:analysisType ],
        [ sh:datatype xsd:string ;
            sh:description "Keywords associated with the dataset." ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path dcat:keyword ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Unique identifier for the dataset, assigned prior to inclusion in bioenergy.org." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:identifier ],
        [ sh:class brc:Plasmid ;
            sh:description "Description of plasmid features, if applicable. This is a multivalued field." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path brc:plasmid_features ],
        [ sh:datatype xsd:string ;
            sh:description "The title of the dataset." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dcterms:title ],
        [ sh:class brc:Funding ;
            sh:description "Funding source(s) for the dataset." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path brc:funding ],
        [ sh:datatype xsd:anyURI ;
            sh:description "URL for the dataset landing page." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path schema1:url ],
        [ sh:description "High-level type of the main content of the dataset." ;
            sh:in ( "AS" "GD" "IM" "ND" "IP" "FP" "SM" "MM" "I" ) ;
            sh:maxCount 1 ;
            sh:order 16 ;
            sh:path brc:datasetType ] ;
    sh:targetClass schema1:Dataset .

schema1:Person a sh:NodeShape ;
    sh:closed true ;
    sh:description "An individual involved in the dataset." ;
    sh:ignoredProperties ( brc:contributorType rdf:type ) ;
    sh:property [ sh:datatype xsd:boolean ;
            sh:description "Indicates if the individual is a primary contact." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:primaryContact ],
        [ sh:description "Affiliation of the individual." ;
            sh:maxCount 1 ;
            sh:or ( [ sh:class brc:BRCOrganization ] [ sh:datatype xsd:string ;
                        sh:nodeKind sh:Literal ] ) ;
            sh:order 3 ;
            sh:path brc:affiliation ],
        [ sh:datatype xsd:anyURI ;
            sh:description "ORCID for the individual. This should include the full URI with prefix, e.g., https://orcid.org/0000-0002-1825-0097." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path brc:orcid ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the individual." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:name ],
        [ sh:datatype xsd:string ;
            sh:description "Email address of the individual." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:email ] ;
    sh:targetClass schema1:Person .

brc:Contributor a sh:NodeShape ;
    sh:closed true ;
    sh:description "An individual who contributed to the dataset in some manner, not necessarily as an author." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:anyURI ;
            sh:description "ORCID for the individual. This should include the full URI with prefix, e.g., https://orcid.org/0000-0002-1825-0097." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path brc:orcid ],
        [ sh:description "Affiliation of the individual." ;
            sh:maxCount 1 ;
            sh:or ( [ sh:class brc:BRCOrganization ] [ sh:datatype xsd:string ;
                        sh:nodeKind sh:Literal ] ) ;
            sh:order 4 ;
            sh:path brc:affiliation ],
        [ sh:description "The contribution type." ;
            sh:in ( "ContactPerson" "DataCollector" "DataCurator" "DataManager" "Distributor" "Editor" "HostingInstitution" "Producer" "ProjectLeader" "ProjectManager" "ProjectMember" "RegistrationAgency" "RegistrationAuthority" "RelatedPerson" "Researcher" "ResearchGroup" "RightsHolder" "Sponsor" "Supervisor" "WorkPackageLeader" "Other" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path brc:contributorType ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the individual." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:name ],
        [ sh:datatype xsd:string ;
            sh:description "Email address of the individual." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path schema1:email ],
        [ sh:datatype xsd:boolean ;
            sh:description "Indicates if the individual is a primary contact." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path brc:primaryContact ] ;
    sh:targetClass brc:Contributor .

brc:Funding a sh:NodeShape ;
    sh:closed true ;
    sh:description "Funding source for the dataset. Each item corresponds to a single award or grant." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class brc:BRCOrganization ;
            sh:description "Details of the funding entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path brc:fundingOrganization ],
        [ sh:datatype xsd:string ;
            sh:description "Title of the award." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:awardTitle ],
        [ sh:datatype xsd:string ;
            sh:description "Award number from the funding entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path brc:awardNumber ],
        [ sh:datatype xsd:anyURI ;
            sh:description "URI for the award. This may be a DOI. Include prefix." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path brc:awardURI ] ;
    sh:targetClass brc:Funding .

brc:Plasmid a sh:NodeShape ;
    sh:closed true ;
    sh:description "Description of plasmid or other molecular vector features." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Origin of replication for the plasmid, e.g., ColE1." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path brc:ori ],
        [ sh:datatype xsd:string ;
            sh:description "Selection markers for the plasmid, e.g, kan." ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path brc:selection_markers ],
        [ sh:datatype xsd:string ;
            sh:description "Description of the plasmid, including any relevant features not captured in other fields." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path brc:description ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the backbone of the plasmid, e.g., pUC19." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:backbone ],
        [ sh:class brc:Organism ;
            sh:description "Organism(s) in which the plasmid replicates. Includes both the scientific name and NCBI Taxonomy ID." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path brc:replicates_in ],
        [ sh:datatype xsd:string ;
            sh:description "Promoters for the plasmid, e.g., T7." ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path brc:promoters ],
        [ sh:class brc:Organism ;
            sh:description "Host organism for the plasmid, e.g., E. coli. Includes both the scientific name and NCBI Taxonomy ID." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path brc:host ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for the plasmid. This must be unique within the dataset." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path brc:id ] ;
    sh:targetClass brc:Plasmid .

brc:RelatedItem a sh:NodeShape ;
    sh:closed true ;
    sh:description "A related publication or item, including cited publications." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:anyURI ;
            sh:description "Identifier or URL for the related item." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brc:relatedItemIdentifier ],
        [ sh:datatype xsd:string ;
            sh:description "Title of the related item." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dcterms:title ],
        [ sh:description "Type of the related item, e.g., JournalArticle." ;
            sh:in ( IAO:0000013 schema1:book schema1:dataset IAO:0000010 schema1:thesis IAO:0000313 schema1:Publication schema1:PresentationDigitalDocument IAO:0000088 schema1:WebPage schema1:WebApplication ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path brc:relatedItemType ] ;
    sh:targetClass brc:RelatedItem .

osti:Affiliation a sh:NodeShape ;
    sh:closed true ;
    sh:description "An affiliation for a person, such as an organization or institution." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Name of the institution or laboratory with which this person is affiliated." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path osti:name ],
        [ sh:datatype xsd:string ;
            sh:description "ROR ID of this affiliation, if any.  Will be validated against ROR organization authority if present." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:ror_id ] ;
    sh:targetClass osti:Affiliation .

osti:AuditLog a sh:NodeShape ;
    sh:closed true ;
    sh:description "Indicates status and information about back-end processing on a given metadata record." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:dateTime ;
            sh:description "Timestamp of the operation detailed in this audit log." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:audit_date ],
        [ sh:datatype xsd:string ;
            sh:description "One or more messages pertaining to the action taken or results of worker processing for this audit log." ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path osti:messages ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates state or notification level of worker action detailed in this audit log.  Generally SUCCESS or FAIL, but may additionally indicate INFO, WARN, or ERROR status messages." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path osti:status ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates the source of the status message, generally the backend process performing the action in question." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path osti:type ] ;
    sh:targetClass osti:AuditLog .

osti:Geolocation a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Describes the shape of this geolocation attribute. (Optional, type may be determined by examination of the points.) Single point in 'points' indicates this is a POINT; two points, indicating NW and SE location, indicate a BOX; any other number of points is assumed to be a POLYGON.  Note that POLYGONs should begin and end on the same point, in order to properly express a 'closed polygon' shape." ;
            sh:in ( "POINT" "BOX" "POLYGON" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path osti:type ],
        [ sh:class osti:Point ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path osti:points ],
        [ sh:datatype xsd:string ;
            sh:description "Optional place name for this location or set of geolocation points." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:label ] ;
    sh:targetClass osti:Geolocation .

osti:Identifier a sh:NodeShape ;
    sh:closed true ;
    sh:description "Values of various identifying numbers, such as DOE contract number, product numbers, ISBN, ISSN, and other various forms of identifying markings or numbers pertaining to the product or metadata." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AUTH_REV" "CN_DOE" "CN_NONDOE" "CODEN" "DOE_DOCKET" "EDB" "ETDE_RN" "INIS_RN" "ISBN" "ISSN" "LEGACY" "NSA" "OPN_ACC" "OTHER_ID" "PATENT" "PROJ_ID" "PROP_REV" "REF" "REL_TRN" "RN" "TRN" "TVI" "USER_VER" "WORK_AUTH" "WORK_PROP" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path osti:type ],
        [ sh:datatype xsd:string ;
            sh:description "Value of this identifier" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:value ;
            sh:pattern "^.{0,100}$" ] ;
    sh:targetClass osti:Identifier .

osti:MediaFile a sh:NodeShape ;
    sh:closed true ;
    sh:description "Metadata information pertaining to a particular media resource associated with this product.  Contains information about its disposition, content, and processing state.  Each individual file is uniquely identified by its `MEDIA_FILE_ID` value." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "Unique identifier for a given MEDIA FILE." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path osti:media_file_id ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates TYPE of media file, detected or set during media processing." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path osti:media_type ;
            sh:pattern "^.{0,1}$" ],
        [ sh:datatype xsd:string ;
            sh:description "If present, the reason why media processing failed, or description of problem encountered processing this particular file." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path osti:processing_exceptions ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Indicates the last date and time this media file was modified." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path osti:date_file_updated ],
        [ sh:datatype xsd:string ;
            sh:description "For PDF media that is PDF/A compliant, the conformance level, generally A, B, or U." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 18 ;
            sh:path osti:pdfa_conformance ],
        [ sh:datatype xsd:string ;
            sh:description "For PDF media files, indicates the version of PDF." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path osti:pdf_version ],
        [ sh:datatype xsd:string ;
            sh:description "For PDF media that is PDF/UA compliant, its compliance level, generally 1 or 2." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path osti:pdfua_part ],
        [ sh:datatype xsd:integer ;
            sh:description "Indicates the number of video tracks in audio-visual media." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path osti:video_tracks ],
        [ sh:datatype xsd:string ;
            sh:description "Either the file name for local files, or URL path to off-site resource." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path osti:url ;
            sh:pattern "^.{0,255}$" ],
        [ sh:datatype xsd:integer ;
            sh:description "If non-zero, indicates unique MEDIA FILE ID this MEDIA FILE is derived from." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path osti:parent_media_file_id ],
        [ sh:datatype xsd:integer ;
            sh:description "For document-based media, the number of printed pages if applicable." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path osti:document_page_count ],
        [ sh:datatype xsd:string ;
            sh:description "For PDF media that is PDF/A compliant, the level of compliance, as a value between 1 and 4." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path osti:pdfa_part ],
        [ sh:datatype xsd:string ;
            sh:description "Indiciates current processing status for this MEDIA FILE. Other values may indicate awaiting additional processing, such as 'OCR', pending text processing." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path osti:status ],
        [ sh:datatype xsd:integer ;
            sh:description "Indicates the E-Link USER ID that attached this MEDIA FILE." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path osti:added_by_user_id ],
        [ sh:datatype xsd:string ;
            sh:description "Mime type describing the MEDIA FILE content." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path osti:mime_type ],
        [ sh:datatype xsd:integer ;
            sh:description "If local file, the file size in bytes." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path osti:file_size_bytes ],
        [ sh:datatype xsd:integer ;
            sh:description "Link to parent MEDIA SET ID." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:media_id ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Indicates the date and time this media file was created." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path osti:date_file_added ],
        [ sh:datatype xsd:integer ;
            sh:description "Indicates the number of subtitle tracks for audio-visual media." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path osti:subtitle_tracks ],
        [ sh:datatype xsd:integer ;
            sh:description "Revision number of this media file, associated with the MEDIA SET" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path osti:revision ],
        [ sh:datatype xsd:string ;
            sh:description "Describes method of file production or association with this media set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path osti:media_source ],
        [ sh:description "Indicates if the file is LOCALLY HOSTED ('L') or OFF-SITE URL ('O')." ;
            sh:in ( "L" "O" ) ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path osti:url_type ;
            sh:pattern "^.{0,1}$" ],
        [ sh:datatype xsd:integer ;
            sh:description "For audio-visual media, the duration of the resource in seconds." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path osti:duration_seconds ],
        [ sh:datatype xsd:string ;
            sh:description "Calculated hash or checksum value of the physical file as applicable, from media processing." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path osti:checksum ] ;
    sh:targetClass osti:MediaFile .

osti:MediaSet a sh:NodeShape ;
    sh:closed true ;
    sh:description "Metadata about files associated with this product.  Summarizes the main media file associated with this product, usually an off-site URL or PDF uploaded to OSTI, with its state, URL if applicable, and other identifying state information pertaining to the media files as a group. Each media set is uniquely identified by its `MEDIA_ID` value." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class osti:MediaFile ;
            sh:description "Array of all files, including original submission of file or URL along with any derived files during processing of media." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path osti:files ],
        [ sh:datatype xsd:string ;
            sh:description "Indicate the current processing of the media file set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path osti:status ],
        [ sh:description "Indicates if this media set's main content is LOCAL or OFF-SITE." ;
            sh:in ( "L" "O" ) ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path osti:media_location ],
        [ sh:datatype xsd:integer ;
            sh:description "Links to Record OSTI_ID value for a Media Set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path osti:osti_id ],
        [ sh:datatype xsd:integer ;
            sh:description "Revision number of this media association set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:revision ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date this media set was most recently modified. (UTC)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path osti:date_updated ],
        [ sh:datatype xsd:string ;
            sh:description "Optional title provided for the given media set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path osti:media_title ;
            sh:pattern "^.{0,500}$" ],
        [ sh:datatype xsd:integer ;
            sh:description "Unique ID for this MEDIA SET." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path osti:media_id ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of pages, if applicable, found in the processing of this file." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path osti:document_page_count ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date this media set was first created. (UTC)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path osti:date_added ],
        [ sh:datatype xsd:dateTime ;
            sh:description "If present, date and time when media association was removed or replaced. (UTC)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path osti:date_valid_end ],
        [ sh:datatype xsd:integer ;
            sh:description "Indicates user ID that added this media set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path osti:added_by ],
        [ sh:datatype xsd:string ;
            sh:description "MIME type description of the file content of this media file. This value is set by OSTI media processing." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path osti:mime_type ],
        [ sh:description "Access limitations are inherited from the parent metadata record at time of association." ;
            sh:in ( "UNL" "OPN" "CPY" "OUO" "PROT" "PDOUO" "ECI" "PDSH" "USO" "LRD" "NAT" "NNPI" "INTL" "PROP" "PAT" "OTHR" "CUI" ) ;
            sh:order 2 ;
            sh:path osti:access_limitations ;
            sh:pattern "^.{0,5}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates the initial primary source of the media set." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path osti:media_source ] ;
    sh:targetClass osti:MediaSet .

osti:Organization a sh:NodeShape ;
    sh:closed true ;
    sh:description "Describes a particular organization associated with the bibliographic record. Organizations may be author collaborations, sponsors, research laboratories, or contributors to the work, as indicated by their associated type. For identification purposes, at least one of either 'name' or 'ror_id' is required for validation.  If ROR ID is specified, it will be validated against the ROR authority at OSTI." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AUTHOR" "CONTRIBUTING" "RESEARCHING" "SPONSOR" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path osti:type ;
            sh:pattern "^.{0,20}$" ],
        [ sh:datatype xsd:string ;
            sh:description "ROR ID for this organization, if any. This value will be validated against the ROR authority." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path osti:ror_id ],
        [ sh:class osti:OrganizationIdentifier ;
            sh:description "List of any identifiers for this Organization.  Only applicable to Sponsoring organizations." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path osti:identifiers ],
        [ sh:description "Indicate the contribution made by this Organization.  Required for CONTRIBUTING 'type'." ;
            sh:in ( "Chair" "DataCollector" "DataCurator" "DataManager" "Distributor" "Editor" "HostingInstitution" "Producer" "ProjectLeader" "ProjectManager" "ProjectMember" "RegistrationAgency" "RegistrationAuthority" "RelatedPerson" "Reviewer" "ReviewAssistant" "ReviewerExternal" "RightsHolder" "StatsReviewer" "Supervisor" "Translator" "WorkPackageLeader" "Other" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path osti:contributor_type ;
            sh:pattern "^.{0,25}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the organization" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:name ;
            sh:pattern "^.{0,800}$" ] ;
    sh:targetClass osti:Organization .

osti:OrganizationIdentifier a sh:NodeShape ;
    sh:closed true ;
    sh:description "One or more identifying numbers or references associated with this organization.  Please note that only sponsoring organizations may have associated identifier values." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "AWARD_DOI" "CN_DOE" "CN_NONDOE" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path osti:type ;
            sh:pattern "^.{0,20}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates the value of this identifier.  May be validated according to its particular type." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:value ] ;
    sh:targetClass osti:OrganizationIdentifier .

osti:Person a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about a particular person involved in the production or maintenance of this record" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "List of any email address(es) associated with this person. Email addresses are validated to be well-formed." ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path osti:email ],
        [ sh:in ( "Chair" "DataCollector" "DataCurator" "DataManager" "Distributor" "Editor" "HostingInstitution" "Producer" "ProjectLeader" "ProjectManager" "ProjectMember" "RegistrationAgency" "RegistrationAuthority" "RelatedPerson" "Reviewer" "ReviewAssistant" "ReviewerExternal" "RightsHolder" "StatsReviewer" "Supervisor" "Translator" "WorkPackageLeader" "Other" ) ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path osti:contributor_type ;
            sh:pattern "^.{0,25}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Middle name or initial of the person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path osti:middle_name ;
            sh:pattern "^.{0,50}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Last (or 'Family') name of this person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path osti:last_name ;
            sh:pattern "^.{0,60}$" ],
        [ sh:datatype xsd:string ;
            sh:description "First (or 'Given') name of the person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:first_name ;
            sh:pattern "^.{0,50}$" ],
        [ sh:datatype xsd:integer ;
            sh:description "OSTI-assigned identifier for this person, if any" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path osti:osti_user_id ],
        [ sh:datatype xsd:string ;
            sh:description "Contact phone number for this person, if available. If provided, must be a valid phone number expression." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path osti:phone ;
            sh:pattern "^.{0,30}$" ],
        [ sh:class osti:Affiliation ;
            sh:description "List of any affiliations for this person.  At least one of either 'name' and/or 'ror_id' is required; if ROR ID is provided, the value will be validated against the OSTI ROR authority." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path osti:affiliations ],
        [ sh:in ( "AUTHOR" "RELEASE" "CONTACT" "CONTRIBUTING" "PROT_CE" "PROT_RO" "SBIZ_PI" "SBIZ_BO" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path osti:type ;
            sh:pattern "^.{0,20}$" ],
        [ sh:datatype xsd:string ;
            sh:description "ORCID (https://orcid.org/) value for this person. ORCID values, if provided, must be of valid format." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path osti:orcid ] ;
    sh:targetClass osti:Person .

osti:Point a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:float ;
            sh:description "Longitude of this point in the geolocation; limited to -180 to 180, inclusive." ;
            sh:maxCount 1 ;
            sh:maxInclusive 180 ;
            sh:minCount 1 ;
            sh:minInclusive -180 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path osti:longitude ],
        [ sh:datatype xsd:float ;
            sh:description "Latitude of this point in the geolocation; limited to -90 to 90, inclusive." ;
            sh:maxCount 1 ;
            sh:maxInclusive 90 ;
            sh:minCount 1 ;
            sh:minInclusive -90 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path osti:latitude ] ;
    sh:targetClass osti:Point .

osti:Record a sh:NodeShape ;
    sh:closed true ;
    sh:description "Defines the bibliographic metadata about a particular work or record. Depending on product type, various elements are permitted, not permitted, or required." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Issue number for journals or other applicable products if any." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path osti:issue ;
            sh:pattern "^.{0,80}$" ],
        [ sh:datatype xsd:string ;
            sh:description "For OpenNET records, the addressee information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 47 ;
            sh:path osti:opn_addressee ],
        [ sh:description "Workflow status of current revision of record." ;
            sh:in ( "R" "SA" "SR" "SO" "SF" "SX" "SV" "X" "D" ) ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path osti:workflow_status ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date contract for this record was awarded." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path osti:contract_award_date ],
        [ sh:datatype xsd:string ;
            sh:description "Exception number for PDOUO access limitation records. Multiple values may be delimited by semi-colons." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 60 ;
            sh:path osti:pdouo_exemption_number ],
        [ sh:datatype xsd:string ;
            sh:description "Concise set of key words for this record" ;
            sh:nodeKind sh:Literal ;
            sh:order 43 ;
            sh:path osti:keywords ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date record first entered the OSTI system. Value internally maintained by E-Link." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path osti:date_metadata_added ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 75 ;
            sh:path osti:report_period_start_date ],
        [ sh:datatype xsd:string ;
            sh:description "The holder of property rights to a patent." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 57 ;
            sh:path osti:patent_assignee ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date record information was released to OSTI, as entered by releasing official." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 72 ;
            sh:path osti:released_to_osti_date ],
        [ sh:datatype xsd:string ;
            sh:description "The previous SBIR/STTR contract number if a Phase III SBIR/STTR report." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 80 ;
            sh:path osti:sbiz_previous_contract_number ;
            sh:pattern "^.{0,14}$" ],
        [ sh:datatype xsd:string ;
            sh:description "\"Any customized infix value for the DOI used when generating a DOI reference. The following characters should be avoided in the infix value: ;/?:@&=+$,.\"" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 31 ;
            sh:path osti:doi_infix ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Date patent was filed with US Patent Office." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 58 ;
            sh:path osti:patent_file_date ],
        [ sh:datatype xsd:string ;
            sh:description "For OpenNET records, the date information was declassified" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 48 ;
            sh:path osti:opn_declassified_date ],
        [ sh:datatype xsd:string ;
            sh:description "Publisher-specific information if applicable" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 41 ;
            sh:path osti:publisher_information ;
            sh:pattern "^.{0,400}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Specific sub-type of the journal article.  For product type JA only. Further qualifies the type of record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path osti:journal_type ;
            sh:pattern "^.{0,2}$" ],
        [ sh:description "Access/distribution limitation codes to describe the distribution rules and limitations for this work." ;
            sh:in ( "UNL" "OPN" "CPY" "OUO" "PROT" "PDOUO" "ECI" "PDSH" "USO" "LRD" "NAT" "NNPI" "INTL" "PROP" "PAT" "OTHR" "CUI" ) ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path osti:access_limitations ;
            sh:pattern "^.{0,5}$" ],
        [ sh:datatype xsd:string ;
            sh:description "Set two-character subject category code values for this record" ;
            sh:nodeKind sh:Literal ;
            sh:order 69 ;
            sh:path osti:subject_category_code ;
            sh:pattern "^.{0,2}$" ],
        [ sh:description "Indicates the OSTI collection type originally creating this record. Maintained internally by E-Link." ;
            sh:in ( "DOE_LAB" "DOE_GRANT" "CHORUS" ) ;
